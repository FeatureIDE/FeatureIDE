					label="FeatureIDE">

				<visibleWhen
						checkEnabled="false">
					<with
							variable="activeMenuSelection">
						<iterate
								ifEmpty="false"
								operator="or">
							<or>
								<test
										args="de.ovgu.featureide.core.featureProjectNature"
										property="de.ovgu.featureide.fm.ui.properties.nature"
										value="true">
								</test>
								<test
										property="de.ovgu.featureide.fm.ui.properties.featuremodel"
										value="true">
								</test>
							</or>
						</iterate>
					</with>
				</visibleWhen>
				<separator
