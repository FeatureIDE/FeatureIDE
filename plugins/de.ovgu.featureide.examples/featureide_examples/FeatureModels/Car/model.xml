<?xml version="1.0" encoding="UTF-8" standalone="no"?>
	<featureModel>
		<properties/>
		<struct>
			<and mandatory="true" name="Car">
				<feature mandatory="true" name="Carbody"/>
				<and name="Radio">
					<or name="Ports">
						<feature name="USB"/>
						<feature name="CD"/>
					</or>
					<and name="Navigation">
						<alt name="DigitalCards">
							<feature name="Europe"/>
							<feature name="USA"/>
						</alt>
						<feature mandatory="true" name="GPSAntenna"/>
					</and>
					<feature name="Bluetooth"/>
				</and>
				<alt mandatory="true" name="Gearbox">
					<feature name="Manual"/>
					<feature name="Automatic"/>
				</alt>
				<feature mandatory="true" name="GearboxTest"/>
			</and>
		</struct>
		<constraints>
			<rule>
				<imp>
					<var>Navigation</var>
					<var>USB</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>Europe</var>
					<var>Gearbox</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>GPSAntenna</var>
					<var>USB</var>
				</imp>
			</rule>
			<rule>
				<conj>
					<var>Carbody</var>
					<var>Gearbox</var>
				</conj>
			</rule>
			<rule>
				<imp>
					<conj>
						<var>Gearbox</var>
						<var>Radio</var>
					</conj>
					<var>Navigation</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>Carbody</var>
					<conj>
						<var>Automatic</var>
						<not>
							<var>Bluetooth</var>
						</not>
					</conj>
				</imp>
			</rule>
		</constraints>
		<calculations Auto="true" Constraints="true" Features="true" Redundant="true" Tautology="true"/>
		<comments/>
		<featureOrder userDefined="false"/>
	</featureModel>
