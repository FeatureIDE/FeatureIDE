				<eq>
					<var>BonusPointsBackOut</var>
					<conj>
						<var>BonusPoints</var>
						<var>BackOut</var>
					</conj>
				</eq>
			</rule>
			<rule>
				<eq>
					<var>BonusPointsStatistics</var>
					<conj>
						<var>BonusPoints</var>
						<var>Statistics</var>
					</conj>
				</eq>
			</rule>
			<rule>
