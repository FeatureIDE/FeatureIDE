					<var>BackOutStatistics</var>
					<conj>
						<var>BackOut</var>
						<var>Statistics</var>
					</conj>
				</eq>
			</rule>
			<rule>
				<eq>
					<var>BonusPointsBackOutStatistics</var>
					<conj>
						<var>BonusPoints</var>
						<conj>
							<var>BackOut</var>
							<var>Statistics</var>
						</conj>
					</conj>
				</eq>
			</rule>
		</constraints>
		<calculations Auto="true" Constraints="true" Features="true" Redundant="true" Tautology="true"/>
		<comments/>
